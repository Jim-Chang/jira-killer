{
  "name": "jira-killer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:chrome": "webpack --config projects/chrome/webpack.config.js",
    "build:content": "ng run content-panel:build:production",
    "build:popup": "ng run popup-panel:build:production",
    "build": "npm run clean && npm-run-all --parallel build:*",
    "watch:chrome": "webpack --config projects/chrome/webpack.config.js -w",
    "watch:content": "ng run content-panel:build:development --watch",
    "watch:popup": "ng run popup-panel:build:development --watch",
    "watch": "npm-run-all --parallel watch:*",
    "crx": "node package-crx.js",
    "test": "ng test",
    "clean": "rimraf ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/elements": "^14.2.5",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "axios": "^1.1.2",
    "chart.js": "^4.1.1",
    "chartjs-plugin-annotation": "^2.1.1",
    "jquery": "^3.6.1",
    "mixpanel-browser": "^2.45.0",
    "moment": "^2.29.4",
    "ngx-build-plus": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular/cli": "~14.2.5",
    "@angular/compiler-cli": "^14.2.0",
    "@types/chrome": "^0.0.197",
    "@types/jasmine": "~4.0.0",
    "@types/jquery": "^3.5.14",
    "@types/mixpanel-browser": "^2.38.0",
    "crx": "^5.0.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.1",
    "typescript": "~4.7.2",
    "webpack-cli": "^4.10.0"
  }
}

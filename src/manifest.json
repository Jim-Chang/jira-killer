{
  "name": "Jira Killer",
  "version": "1.0",
  "description": "Let Jira be cool",
  "manifest_version": 3,
  "permissions": [
      "storage",
      "activeTab",
      "webNavigation"
  ],
  "action": {
      "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]

    }
  ]
}
